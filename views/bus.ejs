                        <li>
                            <a href="/route/<%=route%>" type="button" class="btn btn-primary p-3 m-1">
                                <%=route%>
                            </a>
                            <% if(favorites.includes(route)) {%>
                                <i class="fas fa-heart heart"></i>
                                <%} else{%>
