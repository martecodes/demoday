                <a href="/logout" class="btn btn-secondary m-2">Logout</a>
            </div>
        </div>

        <div class="d-flex justify-content-center">
            <!-- weather app -->
            <div class="container border" style="width: 22rem;">

                <div class="container d-flex justify-content-center">

                    <img src="#" alt="weatherIcon" id="weatherIcon" class="card-img-top">
                    <div class="card-body">

                        <br>
                        <span id="weatherText" class="card-text"></span>
                        <br>
                        <span id="weatherTemp" class="card-text"></span>
                        <br>
                        <span id="weatherLocation" class="card-text"></span>
                    </div>

                </div>
            </div>
        </div>

        <div class="d-flex justify-content-center m-3">
            <a class="btn btn-outline-primary m-2" href="/subway">Subway</a>
            <a class="btn btn-outline-primary m-2" href="/commuterRail">Commuter Rail</a>
        </div>

